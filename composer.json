{
    "name": "zeek/laravel-azure-middleware",
    "description": "Azure Middleware Auth",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=9.0 || >=10.0 || >=11.0",
        "guzzlehttp/guzzle": ">=6.2",
        "microsoft/microsoft-graph": "^2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Root Inc",
            "email": "djewett@rootinc.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RootInc\\LaravelAzureMiddleware\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RootInc\\LaravelAzureMiddleware\\AzureServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
